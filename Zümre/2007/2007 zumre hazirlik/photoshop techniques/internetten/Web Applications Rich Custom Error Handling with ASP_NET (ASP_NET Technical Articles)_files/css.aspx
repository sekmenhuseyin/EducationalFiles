.mnpQuickSearch {
	PADDING-BOTTOM: 7px; PADDING-LEFT: 5px; PADDING-RIGHT: 10px; PADDING-TOP: 5px
}
.mnpQuickSearch A {
	BACKGROUND: none transparent scroll repeat 0% 0%; FONT-WEIGHT: normal
}
.mnpQuickSearch A:visited {
	BACKGROUND: none transparent scroll repeat 0% 0%; FONT-WEIGHT: normal
}
.mnpQuickSearch A:hover {
	BACKGROUND: none transparent scroll repeat 0% 0%; FONT-WEIGHT: normal
}
.mnpSearchBox {
	MARGIN: 2px 0px 1px
}
.mnpSearchButton {
	MARGIN-LEFT: 7px; PADDING-LEFT: 1px
}
.syncBarNext {
	BACKGROUND: #cccccc; BORDER-BOTTOM: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #cccccc 1px solid; CURSOR: hand; HEIGHT: 15px; MARGIN-LEFT: 7px; WIDTH: 15px
}
.syncBarPrevious {
	BACKGROUND: #cccccc; BORDER-BOTTOM: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #cccccc 1px solid; CURSOR: hand; HEIGHT: 15px; MARGIN-LEFT: 7px; WIDTH: 15px
}
.mnpQuickSearch {
	FONT: 7.5pt Verdana
}
.mnpSearchBox {
	FONT: 11px Verdana
}
.mnpSearchScopes {
	FONT: 11px Tahoma
}
.mnpSearchButton {
	FONT: 11px Tahoma
}
.mnpMenuTop {
	BACKGROUND: #f1f1f1; BORDER-BOTTOM: #999999 0px solid; BORDER-LEFT: #999999 0px solid; BORDER-RIGHT: #999999 1px solid; BORDER-TOP: #999999 0px solid; FONT-SIZE: small; PADDING-TOP: 2px
}
.mnpMenuSub {
	BACKGROUND: #f1f1f1; BORDER-BOTTOM: #999999 1px solid; BORDER-LEFT: #999999 1px solid; BORDER-RIGHT: #999999 1px solid; BORDER-TOP: #999999 1px solid; PADDING-BOTTOM: 2px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 2px; POSITION: absolute
}
.mnpMenuRow {
	BORDER-BOTTOM: 1px solid; BORDER-LEFT: 1px solid; BORDER-RIGHT: 1px solid; BORDER-TOP: 1px solid; COLOR: black; CURSOR: hand; MARGIN: 0px 2px; PADDING-BOTTOM: 3px; PADDING-LEFT: 6px; PADDING-RIGHT: 15px; PADDING-TOP: 1px
}
.mnpMenuRow A {
	BACKGROUND: none transparent scroll repeat 0% 0%; COLOR: black; FONT-WEIGHT: normal; TEXT-DECORATION: none
}
.mnpMenuRow A:visited {
	BACKGROUND: none transparent scroll repeat 0% 0%; COLOR: black; FONT-WEIGHT: normal; TEXT-DECORATION: none
}
.mnpMenuRow A:hover {
	BACKGROUND: none transparent scroll repeat 0% 0%; COLOR: black; FONT-WEIGHT: normal; TEXT-DECORATION: none
}
.mnpMenuLabel {
	BACKGROUND: #f1f1f1; COLOR: black; PADDING-BOTTOM: 4px; PADDING-LEFT: 4px; PADDING-RIGHT: 2px; PADDING-TOP: 2px
}
.mnpMenuBorder {
	BACKGROUND: #999999; HEIGHT: 1px; MARGIN: 2px 0px; OVERFLOW: hidden
}
.mnpMenuArrow {
	MARGIN-TOP: 4px; POSITION: absolute
}
.mnpMenuScroller {
	BORDER-BOTTOM: 1px solid; BORDER-LEFT: 1px solid; BORDER-RIGHT: 1px solid; BORDER-TOP: 1px solid; HEIGHT: 19px; MARGIN: 0px 2px; OVERFLOW: hidden; PADDING-TOP: 4px; TEXT-ALIGN: center
}
.mnpMenuScrollArea {
	BACKGROUND: none transparent scroll repeat 0% 0%; OVERFLOW: hidden
}
.mnpAds {
	BACKGROUND: none transparent scroll repeat 0% 0%
}
.mnpAds A {
	BACKGROUND: none transparent scroll repeat 0% 0%
}
.mnpAds A:link {
	BACKGROUND: none transparent scroll repeat 0% 0%
}
.mnpAds A:visited {
	BACKGROUND: none transparent scroll repeat 0% 0%
}
.mnpAds A:hover {
	BACKGROUND: none transparent scroll repeat 0% 0%
}
.mnpInherit {
	BACKGROUND: none transparent scroll repeat 0% 0%
}
.mnpMenuLabel {
	FONT: bold 70% Verdana
}
.mnpMenuRow {
	FONT: 70% Verdana
}
BODY {
	MARGIN: 0px
}
